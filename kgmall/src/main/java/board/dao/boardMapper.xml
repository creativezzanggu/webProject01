<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boardSQL">
	<select id="getTitleList" resultType="String">
		select title from (select rownum, title from notice order by rownum desc) where rownum <![CDATA[<=]]> 7
	</select>
</mapper>




















